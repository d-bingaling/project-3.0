{
    "configuration": [

        {
            "legend": "Cast 3.0",
            "open": true,
            "widgets": [{
                    "number_engagement_fees": {
                        "acl": "only_internal",
                        "args": {
                            "controls": [
                                "engagement_production_total_jinja"
                            ],
                            "dynamic": true
                        },
                        "label": "Number of engagement fees payable",
                        "widget_type": "numeric"
                    }
                },
                {
                    "additional_multi_episodic": {
                        "acl": "only_internal",
                        "args": {
                            "controls": [
                                "engagement_production_total_jinja",
                                "total_guaranteed_minimum",
                                "row_can",
                                "usa_basic",
                                "uk_video",
                                "us_video",
                                "row_video",
                                "uk_dto_dtr",
                                "us_dto_dtr",
                                "row_dto_dtr",
                                "uk_svod_7_yrs",
                                "usa_svod_7_yrs"
                            ],
                            "dynamic": true
                        },
                        "label": "Additional multi-episodic engagement fees payable",
                        "widget_type": "numeric"
                    }
                },
                {
                    "hr": {
                        "acl": "only_internal",
                        "args": {
                            "document": "<hr />"
                        },
                        "widget_type": "prt"
                    }
                },
                {
                    "engagement_fee": {
                        "acl": "only_internal",
                        "args": {
                            "controls": [
                                "engagement_production_total_jinja",
                                "total_guaranteed_minimum",
                                "row_can",
                                "usa_basic",
                                "uk_video",
                                "us_video",
                                "row_video",
                                "uk_dto_dtr",
                                "us_dto_dtr",
                                "row_dto_dtr",
                                "uk_svod_7_yrs",
                                "usa_svod_7_yrs"
                            ],
                            "dynamic": true,
                            "placeholder": "--.--"
                        },
                        "label": "Engagement fee ",
                        "widget_type": "numeric"
                    }
                },
                {
                    "hr": {
                        "acl": "only_internal",
                        "args": {
                            "document": "<hr />"
                        },
                        "widget_type": "prt"
                    }
                },
                {
                    "production_day_payments": {
                        "acl": "only_internal",
                        "args": {
                            "controls": [
                                "engagement_production_total_jinja",
                                "total_guaranteed_minimum",
                                "row_can",
                                "usa_basic",
                                "uk_video",
                                "us_video",
                                "row_video",
                                "uk_dto_dtr",
                                "us_dto_dtr",
                                "row_dto_dtr",
                                "uk_svod_7_yrs",
                                "usa_svod_7_yrs"
                            ],
                            "dynamic": true,
                            "placeholder": "--.--"
                        },
                        "label": "Number of production day payments",
                        "widget_type": "numeric"
                    }
                },
                {
                    "hr": {
                        "acl": "only_internal",
                        "args": {
                            "document": "<hr />"
                        },
                        "widget_type": "prt"
                    }
                },
                {
                    "section_g": {
                        "acl": "deal_memo",
                        "args": {
                            "document": "<h2>Engagement fee</h2>"
                        },
                        "label": "",
                        "widget_type": "prt"
                    }
                },
                {
                    "engagement_production_total_jinja": {
                        "acl": "deal_memo",
                        "args": {
                            "controls": [
                                "total_guaranteed_minimum",
                                "row_can",
                                "usa_basic",
                                "uk_video",
                                "us_video",
                                "row_video",
                                "uk_dto_dtr",
                                "us_dto_dtr",
                                "row_dto_dtr",
                                "uk_svod_7_yrs",
                                "usa_svod_7_yrs"
                            ],
                            "template": "\u00a3{{ (deal_memo.meta.get('number_engagement_fees', decimal.Decimal('0')) + deal_memo.meta.get('additional_multi_episodic', decimal.Decimal('0'))) * deal_memo.meta.get('engagement_fee', decimal.Decimal('0')) + (deal_memo.meta.get('production_day_payments', decimal.Decimal('0')) * decimal.Decimal(\"61.50\")) }}"
                        },
                        "label": "Total of engagement and production fees",
                        "widget_type": "jinja2"
                    }
                },
                {
                    "hr": {
                        "acl": "deal_memo",
                        "args": {
                            "document": "<hr />"
                        },
                        "widget_type": "prt"
                    }
                },

                {
                    "section_i": {
                        "acl": "only_internal",
                        "args": {
                            "document": "<h2>Other additional uses</h2>"
                        },
                        "widget_type": "prt"
                    }
                },
                {
                    "usa_basic": {
                        "acl": "default",
                        "args": {
                            "template": "{% if 1 in deal_memo.project_record.project.settings['percentages_additional_uses'] %}<p>USA Basic Cable 10%</p> <strong>{{ (((deal_memo.meta.get('number_engagement_fees', decimal.Decimal('0')) + deal_memo.meta.get('additional_multi_episodic', decimal.Decimal('0'))) * deal_memo.meta.get('engagement_fee', decimal.Decimal('0')) + (deal_memo.meta.get('production_day_payments', decimal.Decimal('0'))) * decimal.Decimal(\"61.50\")) * decimal.Decimal(\"0.10\")).quantize(decimal.Decimal(\".01\")) }}</strong>{% endif %}"
                        },

                        "widget_type": "jinja2"
                    }
                },
                {
                    "uk_video": {
                        "acl": "only_internal",
                        "args": {
                            "template": "{% if 2 in deal_memo.project_record.project.settings['percentages_additional_uses'] %}<p>UK Video 5%</p>{{ (((deal_memo.meta.get('number_engagement_fees', decimal.Decimal('0')) + deal_memo.meta.get('additional_multi_episodic', decimal.Decimal('0'))) * deal_memo.meta.get('engagement_fee', decimal.Decimal('0')) + (deal_memo.meta.get('production_day_payments', decimal.Decimal('0'))) * decimal.Decimal(\"61.50\")) * decimal.Decimal(\"0.05\")).quantize(decimal.Decimal(\".01\")) }}{% endif %}"
                        },
                        "widget_type": "jinja2"
                    }
                },
                {
                    "us_video": {
                        "acl": "only_internal",
                        "args": {
                            "template": "{% if 3 in deal_memo.project_record.project.settings['percentages_additional_uses'] %}<p>US Video 4%</p>{{ (((deal_memo.meta.get('number_engagement_fees', decimal.Decimal('0')) + deal_memo.meta.get('additional_multi_episodic', decimal.Decimal('0'))) * deal_memo.meta.get('engagement_fee', decimal.Decimal('0')) + (deal_memo.meta.get('production_day_payments', decimal.Decimal('0'))) * decimal.Decimal(\"61.50\")) * decimal.Decimal(\"0.04\")).quantize(decimal.Decimal(\".01\")) }}{% endif %}"
                        },
                        "widget_type": "jinja2"
                    }
                }
            ]
        }
    ],
    "pre_population": [],
    "duplicate_engagement": [

    ],
    "required": [],
    "required_to_send": []
}